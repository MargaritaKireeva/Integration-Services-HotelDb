                <property
                  dataType="System.String"
                  description="Команда SQL для выполнения."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    a.ID AS AccommodationID,
    a.ClientID,
    rt.TypeName, 
    CAST(CONVERT(VARCHAR(8), a.ArrivalDate, 112) AS BIGINT) AS ArrivalDateBigint,
    CAST(CONVERT(VARCHAR(8), a.DepartureDate, 112) AS BIGINT) AS DepartureDateBigint,
    CASE 
        WHEN a.BookingID IS NULL THEN CAST(0 AS BIT)  
        ELSE CAST(1 AS BIT)                         
    END AS IsBooking
FROM 
    AccommodationInfo a
JOIN 
    Room r ON a.RoomID = r.ID
JOIN 
    RoomType rt ON r.TypeID = rt.ID;</property>
